<h2>create picture</h2>



	
<%= form_with model: @picture do |form|%>

<%= form.hidden_field :product_id, value: params[:product_id] %>
<%#= form.hidden_field :employee_id, value: params[:employee_id] %>

	<div>
		<%= form.label :name %><br>
		<%= form.text_field :name %>

	</div>
	
	<div>
		<%= form.submit %>
	</div>
<% end %>
<%= link_to "back" , pictures_path %>
